@article{ WOS:000707909500004,
Author = {Zhang, Yi-hai and Ma, Hai-tao and Yu, Zheng-xing},
Title = {Application of the method for prediction of the failure location and
   time based on monitoring of a slope using synthetic aperture radar},
Journal = {ENVIRONMENTAL EARTH SCIENCES},
Year = {2021},
Volume = {80},
Number = {21},
Month = {NOV},
Abstract = {Synthetic aperture radar (SAR) technology has been widely used in
   landslide deformation monitoring in the past decade. It has the
   advantages of high monitoring accuracy, a wide range, and flexibility
   allowing all-weather continuous monitoring. The self-developed S-SAR
   synthetic aperture radar (slope radar) is the first completely
   domestic-made radar used in slope deformation monitoring equipment in
   China, and its performance and technical parameters are equal to or
   better than similar products made abroad. The characteristics of
   deformation data collected by S-SAR slope radar deployed in the front
   open pit mine are analyzed to further develop a spatio-temporal
   landslide prediction method which is applicable to the massive
   monitoring data within the monitoring range of slope radar. The
   intersection points of short-term moving average velocity curve and
   long-term moving average velocity curve of slope deformation, which are
   onset of acceleration (OOA) and termination of acceleration (TOA). When
   OOA occurs, the deformation will accelerate, and when TOA occurs, the
   deformation will tend to stabilize. The OOA can identify areas at risk
   in the monitored area before failure, so that the spatial position
   prediction of landslide early warning can be realized. Based on the
   assumptions of the inverse velocity method, a T-log (t) logarithmic
   model is established, and the updated monitoring data are corrected to
   approximate the time of failure, thus improving the accuracy of
   landslide location and time prediction. In an open-pit copper mine in
   Serbia, the accurate prediction of landslide location and time has been
   successfully applied, guaranteeing safe mining.},
