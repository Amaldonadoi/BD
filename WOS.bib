@article{ WOS:001310138100019,
Author = {Jiang, Xiaowei and Shi, Wenbing and Liang, Feng and Gui, Jingjing and
   Li, Jiawei},
Title = {InSAR-derived surface deformation characteristics and mining subsidence
   parameters in mountain coal mines},
Journal = {JOURNAL OF MOUNTAIN SCIENCE},
Year = {2024},
Volume = {21},
Number = {9},
Pages = {3139-3156},
Month = {SEP},
Abstract = {Mining-induced surface deformation disrupts ecological balance and
   impedes economic progress. This study employs SBAS-InSAR with 107-view
   of ascending and descending SAR data from Sentinel-1, spanning February
   2017 to September 2020, to monitor surface deformation in the Fa'er Coal
   Mine, Guizhou Province. Analysis on the surface deformation time series
   reveals the relationship between underground mining and surface shifts.
   Considering geological conditions, mining activities, duration, and
   ranges, the study determines surface movement parameters for the coal
   mine. It asserts that mining depth significantly influences surface
   movement parameters in mountainous mining areas. Increasing mining depth
   elevates the strike movement angle on the deeper side of the burial
   depth by 22.84 degrees, while decreasing by 7.74 degrees on the
   shallower side. Uphill movement angles decrease by 4.06 degrees, while
   downhill movement angles increase by 15.71 degrees. This emphasizes the
   technology's suitability for local mining design, which lays the
   groundwork for resource development, disaster prevention, and ecological
   protection in analogous contexts.},
