@article{ WOS:000873683200001,
Author = {Liu, Maoqi and Long, Sichun and Wu, Wenhao and Liu, Ping and Zhang, Liya
   and Zhu, Chuanguang},
Title = {Instability Monitoring and Numerical Analysis of Typical Coal Mines in
   Southwest China Based on DS-InSAR},
Journal = {SENSORS},
Year = {2022},
